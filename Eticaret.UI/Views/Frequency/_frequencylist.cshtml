@model RadioApiViewListModel

@foreach (var item in Model.RadioApies)
{
    @foreach (var frequency in item.Frequencys)
    {

        <div class="col-6 col-sm-4 col-lg-3" id="frequency">
            <div class="product">
                <div class="product__img">
                    <img src="/upload/frekanslar/@frequency.imgPath" alt="@frequency.cityName">
                </div>
                <h3 class="product__title"><a href="@Url.RouteUrl("Frequency","frekaslar")">@frequency.cityName</a></h3>
                <span class="product__price">@item.title</span>
            </div>
        </div>
     }

}

